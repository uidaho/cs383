projMemberList[] : string
}
class WebMod {
}
WebMod -up-|> User
Profile *-left- "1" User
SettingsManager o-down- "1" Profile
SettingsManager -right- UpdateProfileData : "Update Data"
Authentication o-down- "1" User
Authentication -right- SettingsManager : "Authenticate"
@enduml